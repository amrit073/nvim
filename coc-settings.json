{ 
"coc.preferences.formatOnSaveFiletypes": ["css", "markdown"],
"languageserver": {
  "golang": {
    "command": "gopls",
    "filetypes": ["go"],
    "initializationOptions": {
      "gocodeCompletionEnabled": true,
      "diagnosticsEnabled": true,
      "formatTool": "gofmt"
    },
    "rootPatterns": ["go.mod"]
  }
}
}
